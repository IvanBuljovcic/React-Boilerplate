    "node": ">=10.7.0",
    "npm": ">=6.2.0"
  },
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,css,scss,less}": [
        "npm run prettier:write"
      ],
      "*.{js,jsx}": [
        "npm run eslint",
        "git add"
      ]
    },
    "ignore": [
      "node_modules",
      "webpack.**",
      "dist/**"
    ]
  },
  "main": "index.js",
  "name": "react-boilerplate",
  "repository": "git@github.com:IvanBuljovcic/React-Boilerplate.git",
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "npm run clean && npm run prettier:write && npm run prod",
    "clean": "rimraf ./dist/**/*",
    "debug:dev": "npm run debug -- --env.mode development",
    "debug:prod": "npm run debug -- --env.mode production",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack -- --env.mode development --env.presets postcss",
    "eslint": "eslint -c ./.eslintrc.json \"./src/**/*.{js,jsx}\"",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged && npm run test",
    "prettier:show": "prettier --config ./prettier.config.json \"src/**/*.{js,jsx,ts,css,scss,less}\"",
    "prettier:write": "prettier --config ./prettier.config.json --write \"src/**/*.{js,jsx,ts,css,scss,less}\"",
    "prod:server": "npm run build && npm run prod:open",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod": "npm run webpack -- --env.mode production --env.presets postcss",
    "prod:open": "babel-node build-utils/distServer.js",
    "start": "webpack-dev-server --hot --env.mode development --env.presets eslint  postcss",
    "start:storybook": "concurrently \"npm run dev:server -- --env.presets postcss\" \"npm run storybook\"",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --config=jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm run test -- --update",
    "webpack:dashboard": "webpack-dashboard -- webpack-dev-server --hot",
    "webpack": "webpack"
  },
  "version": "1.0.0"
}
