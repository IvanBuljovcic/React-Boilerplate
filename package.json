    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1"
  },
  "description": "Boilerplate for starting new projects in React",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/node": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "autoprefixer": "^9.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-jsx-display-if": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.24.6",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "lodash-es": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-discard-comments": "^4.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-map": "^0.9.0",
    "postcss-nested": "^3.0.0",
    "postcss-sorting": "^4.0.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.4",
    "react-imported-component": "^4.6.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.2"
  },
  "engines": {
    "node": ">=10.7.0",
    "npm": ">=6.2.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "main": "index.js",
  "name": "react-boilerplate",
  "repository": "git@gitlab.internal.roundglobe.tech:andrija/react-boilerplate.git",
  "scripts": {
    "build": "$npm_execpath run clean && $npm_execpath run prettier:write && $npm_execpath run prod",
    "clean": "rimraf ./dist/**/*",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "debug:dev": "$npm_execpath run debug -- --env.mode development",
    "debug:prod": "$npm_execpath run debug -- --env.mode production",
    "dev": "$npm_execpath run webpack -- --env.mode development",
    "dev:bundle": "$npm_execpath run dev:postcss",
    "dev:lint": "$npm_execpath run dev -- --env.presets eslint",
    "dev:postcss": "$npm_execpath run dev -- --env.presets postcss",
    "dev:server": "$npm_execpath run webpack:server -- --env.mode development --env.presets eslint",
    "dev:watch": "$npm_execpath run dev -- --watch",
    "eslint": "eslint -c ./.eslintrc.json \"./src/**/*.{js,jsx}\"",
    "lint-staged": "lint-staged",
    "open:dist": "babel-node build-utils/distServer.js",
    "precommit": "lint-staged",
    "prettier": "prettier",
    "prettier:show": "$npm_execpath prettier -- --config ./prettier.config.json \"src/**/*.{js,jsx,ts,css,scss,less}\"",
    "prettier:write": "$npm_execpath prettier -- --config ./prettier.config.json --write \"src/**/*.{js,jsx,ts,css,scss,less}\"",
    "prod": "$npm_execpath run webpack -- --env.mode production",
    "prod:analyze": "$npm_execpath run prod -- --env.presets analyze",
    "prod:compress": "$npm_execpath run prod -- --env.presets compress",
    "prod:server": "$npm_execpath run build && $npm_execpath run open:dist",
    "start": "$npm_execpath run dev:server -- --env.presets postcss",
    "webpack": "webpack",
    "webpack:server": "webpack-dev-server --hot"
  },
  "version": "1.0.0"
}
